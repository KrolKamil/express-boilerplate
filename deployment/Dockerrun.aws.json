{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "redis",
      "image": "redis",
      "memory": 256,
      "essential": true
    },
    {
      "name": "api",
      "image": "redis",
      "memory": 256,
      "command": ["api"],
      "essential": true,
      "links": [
        "redis"
      ],
      "portMappings": [
        {
          "hostPort":80,
          "containerPort":1337
        }
      ]
    }
  ]s
}